{
    "name": "hono-react-cloudflare-worker-starter",
    "packageManager": "bun@1.2.18",
    "author": {
        "name": "Ifty",
        "email": "ifty64bit@gmail.com"
    },
    "license": "MIT",
    "keywords": [
        "cloudflare",
        "worker",
        "hono",
        "react",
        "vite"
    ],
    "workspaces": [
        "./api",
        "./web",
        "./shared"
    ],
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "test": "turbo test",
        "type-check": "turbo type-check",
        "clean": "turbo clean",
        "deploy": "bun run build && cd api && bun run deploy",
        "deploy:staging": "bun run build && cd api && wrangler deploy --dry-run",
        "cf-typegen": "cd api && bun run cf-typegen"
    },
    "devDependencies": {
        "rimraf": "^6.0.1",
        "@types/bun": "latest",
        "turbo": "^2.5.5"
    }
}
